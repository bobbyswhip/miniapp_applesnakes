# App Configuration
NEXT_PUBLIC_APP_NAME="AppleSnakes NFT"

# WalletConnect Project ID
# Get yours at https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# IPFS Configuration
# Your IPFS base directory hash (without ipfs:// prefix)
# Example: If your images are at ipfs://QmXXX/1.png, use QmXXX
NEXT_PUBLIC_IPFS_BASE_URI=QmYourBaseHashHere

# Alchemy API Key - Base Mainnet
# Used for fast NFT indexing and querying
# Get yours at https://www.alchemy.com/
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key

# Coinbase Developer Platform RPC API Key
# Used as primary RPC endpoint for better performance
# Get yours at https://portal.cdp.coinbase.com/
NEXT_PUBLIC_COINBASE_RPC_KEY=your_coinbase_rpc_key
